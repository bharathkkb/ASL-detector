{
  "extends": [
    "airbnb",
    "@vikr01",
    "prettier",
    "prettier/react",
    "prettier/flowtype"
  ],
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0,
    "import/prefer-default-export": 0,
    "no-await-in-loop": 0,
    "react/destructuring-assignment": 0,
    "react/sort-comp": 0
  },
  "overrides": [
    {
      "files": ["frontend/src/**/*"],
      "env": {
        "browser": true
      }
    }
  ]
}
